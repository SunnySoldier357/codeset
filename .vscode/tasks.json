{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "codeset build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/codeset/codeset.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "codeset publish",
            "type": "shell",
            "command": "./.vscode/Scripts/publish.sh"
        }
    ]
}